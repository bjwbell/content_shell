node {
   stage 'Checkout'
   sh "rm -rf chrome/"
   sh "git clone --depth=1 --no-single-branch ssh://APKAJHXR727PTXMIECSQ@git-codecommit.us-east-1.amazonaws.com/v1/repos/chrome"
   sh "cd chrome/; git fetch --depth=1 origin master"
   sh "cd chrome/; git checkout ironframe"
   stage 'Diff'
   sh "cd chrome/; git diff origin/master...origin/ironframe > ../../../ironframe/diffs/ironframe-`date -Iminutes`.diff"
   stage 'Push'
   sh "cd ../../ironframe; git pull origin master"
   sh "cd ../../ironframe; git add diffs/*.diff"
   sh "cd ../../ironframe; git commit -m \"Add `date -Iminutes` diff\""
   sh "cd ../../ironframe; git push origin master"
}