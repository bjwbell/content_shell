node {
   stage 'Checkout'
   sh "rm -rf chrome/"
   sh "git clone --no-single-branch ssh://APKAJHXR727PTXMIECSQ@git-codecommit.us-east-1.amazonaws.com/v1/repos/chrome"
   sh "cd chrome/; git fetch origin master"
   sh "cd chrome/; git checkout ironframe"
   stage 'Diff'
   sh "cd chrome/; git format-patch master --stdout > ../../../ironframe/diffs/ironframe-`date -Iminutes`.patch"
   stage 'Push'
   sh "cd ../../ironframe; git pull origin master"
   sh "cd ../../ironframe; git add diffs/*.patch"
   sh "cd ../../ironframe; git commit -m \"Add `date -Iminutes`.patch\""
   sh "cd ../../ironframe; git push origin master"
}