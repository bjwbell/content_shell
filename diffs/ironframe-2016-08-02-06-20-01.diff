-void DefaultHeaderPainter::Init(
-    views::Widget* frame,
-    views::View* header_view,
-    FrameCaptionButtonContainerView* caption_button_container) {
-  DCHECK(frame);
-  DCHECK(header_view);
-  DCHECK(caption_button_container);
-  frame_ = frame;
-  view_ = header_view;
-  caption_button_container_ = caption_button_container;
