+    const void* cmd_data) {
+  const gles2::cmds::ScheduleCALayerFilterEffectsCHROMIUMImmediate& c =
+      *static_cast<
+          const gles2::cmds::ScheduleCALayerFilterEffectsCHROMIUMImmediate*>(
+          cmd_data);
