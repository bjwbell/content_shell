-base::Time ParseLastVisitDate(const std::string& date_string) {
-  int64_t date = 0;
-  if (!base::StringToInt64(date_string, &date))
-    return base::Time::UnixEpoch();
-  return base::Time::FromInternalValue(date);
-}
