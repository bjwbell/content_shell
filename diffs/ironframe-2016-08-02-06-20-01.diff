-      inactive_frame_color_(kDefaultFrameColor),
-      caption_button_container_(NULL),
-      painted_height_(0),
-      mode_(MODE_INACTIVE),
-      initial_paint_(true),
-      activation_animation_(new gfx::SlideAnimation(this)) {}
