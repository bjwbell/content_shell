+  FrameCaptionButton* buttons[] = {minimize_button_, size_button_,
+                                   close_button_};
+  for (size_t i = 0; i < arraysize(buttons); ++i) {
+    if (buttons[i]->icon() == icon)
+      buttons[i]->SetImage(icon, FrameCaptionButton::ANIMATE_NO, icon_image_id);
+  }
