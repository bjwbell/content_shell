-  canvas->DrawStringRectWithFlags(
-      frame_->widget_delegate()->GetWindowTitle(), GetTitleFontList(),
-      kTitleTextColor, title_bounds, gfx::Canvas::NO_SUBPIXEL_RENDERING);
