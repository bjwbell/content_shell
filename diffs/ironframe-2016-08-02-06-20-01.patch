@@ -0,0 +1,62 @@
+<html requestVisibility=1 style="transform:translateZ(0)">
+  <body>
+    <script>
+      var enabled=true;
+      var numAlerts = 0;
+      //top.document.getElementById("result").innerText = "PASS";      
+    </script>
+    <img src="paypal.png" style="position:absolute;top:0;left:0">
+    <script>
+      var prev;
+      var prevMsg="";
+      var v={};
+      var visible=false;
+      
+      var update = function(){
+      top.document.getElementById("result").innerText = "T2";
+          if(v.visible.width < 333 || v.visible.height< 270 ) 
+          {
+               visible=false;
+          } else {
+               visible=true;
+          }
+          if (numAlerts < 2) {
+              alert("visible: " + v.visible.width + "x" + v.visible.height);
+              numAlerts++;
+          }
+          if (window.testRunner) {
+              testRunner.dumpAsText();
+              if (window.internals) {
+                  if (visible == true) {
+                      top.document.getElementById("result").innerText = "PASS";
+                  } else {
+                      top.document.getElementById("result").innerText = "FAIL";
+                  }                  
+              }
+          }
+
+	  if(!visible) {
+	  } else {
+	      if(visible && Date.now()-prev < 2000) {
+	          setTimeout(update, 2250);
+	      } else {
+	      }
+	  }
+	 }
+	 function go() {
+             //top.document.getElementById("result").innerText = "T4";
+	     document.documentElement.addEventListener('requestedvisibility', function(e) {
+                 top.document.getElementById("result").innerText = "T3";
+	         if(e.message!=prevMsg){
+		     prev=e.timeStamp;
+		     prevMsg=e.message;
+	         }
+	         v = JSON.parse(e.message);
+	         v.innerHeight = innerHeight;
+	         v.innerWidth  = innerWidth;
+	         v.ancestorOrigins = document.location.ancestorOrigins;	         
+	         update();	     
+	     });             
+	 }
+	 if(enabled) { window.onload=go; }
+	</script>
